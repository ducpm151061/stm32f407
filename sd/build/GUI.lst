 262:Core/Src/GUI.c ****  * @name       :void gui_circle(int xc, int yc,u16 c,int r, int fill)
 263:Core/Src/GUI.c ****  * @date       :2018-08-09 
 264:Core/Src/GUI.c ****  * @function   :Draw a circle of specified size at a specified location
 265:Core/Src/GUI.c ****  * @parameters :xc:the x coordinate of the Circular center 
 266:Core/Src/GUI.c ****                 yc:the y coordinate of the Circular center 
 267:Core/Src/GUI.c **** 								r:Circular radius
 268:Core/Src/GUI.c **** 								fill:1-filling,0-no filling
 269:Core/Src/GUI.c ****  * @retvalue   :None
 270:Core/Src/GUI.c **** ******************************************************************************/  
 271:Core/Src/GUI.c **** void gui_circle(int xc, int yc,u16 c,int r, int fill)
 272:Core/Src/GUI.c **** {
 273:Core/Src/GUI.c **** 	int x = 0, y = r, yi, d;
 274:Core/Src/GUI.c **** 
 275:Core/Src/GUI.c **** 	d = 3 - 2 * r;
 276:Core/Src/GUI.c **** 
 277:Core/Src/GUI.c **** 
 278:Core/Src/GUI.c **** 	if (fill) 
 279:Core/Src/GUI.c **** 	{
 280:Core/Src/GUI.c **** 		// 如果填充（画实心圆）
 281:Core/Src/GUI.c **** 		while (x <= y) {
 282:Core/Src/GUI.c **** 			for (yi = x; yi <= y; yi++)
 283:Core/Src/GUI.c **** 				_draw_circle_8(xc, yc, x, yi, c);
 284:Core/Src/GUI.c **** 
 285:Core/Src/GUI.c **** 			if (d < 0) {
 286:Core/Src/GUI.c **** 				d = d + 4 * x + 6;
 287:Core/Src/GUI.c **** 			} else {
 288:Core/Src/GUI.c **** 				d = d + 4 * (x - y) + 10;
 289:Core/Src/GUI.c **** 				y--;
 290:Core/Src/GUI.c **** 			}
 291:Core/Src/GUI.c **** 			x++;
 292:Core/Src/GUI.c **** 		}
 293:Core/Src/GUI.c **** 	} else 
 294:Core/Src/GUI.c **** 	{
 295:Core/Src/GUI.c **** 		// 如果不填充（画空心圆）
 296:Core/Src/GUI.c **** 		while (x <= y) {
 297:Core/Src/GUI.c **** 			_draw_circle_8(xc, yc, x, y, c);
 298:Core/Src/GUI.c **** 			if (d < 0) {
