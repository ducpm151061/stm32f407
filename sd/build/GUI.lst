 153:Core/Src/GUI.c ****  * @name       :void LCD_DrawLine(u16 x1, u16 y1, u16 x2, u16 y2)
 154:Core/Src/GUI.c ****  * @date       :2018-08-09 
 155:Core/Src/GUI.c ****  * @function   :Draw a line between two points
 156:Core/Src/GUI.c ****  * @parameters :x1:the bebinning x coordinate of the line
 157:Core/Src/GUI.c ****                 y1:the bebinning y coordinate of the line
 158:Core/Src/GUI.c **** 								x2:the ending x coordinate of the line
 159:Core/Src/GUI.c **** 								y2:the ending y coordinate of the line
 160:Core/Src/GUI.c ****  * @retvalue   :None
 161:Core/Src/GUI.c **** ********************************************************************/
 162:Core/Src/GUI.c **** void LCD_DrawLine(u16 x1, u16 y1, u16 x2, u16 y2)
 163:Core/Src/GUI.c **** {
 164:Core/Src/GUI.c **** 	u16 t; 
 165:Core/Src/GUI.c **** 	int xerr=0,yerr=0,delta_x,delta_y,distance; 
 166:Core/Src/GUI.c **** 	int incx,incy,uRow,uCol; 
 167:Core/Src/GUI.c **** 
 168:Core/Src/GUI.c **** 	delta_x=x2-x1; //计算坐标增量 
 169:Core/Src/GUI.c **** 	delta_y=y2-y1; 
 170:Core/Src/GUI.c **** 	uRow=x1; 
 171:Core/Src/GUI.c **** 	uCol=y1; 
 172:Core/Src/GUI.c **** 	if(delta_x>0)incx=1; //设置单步方向 
 173:Core/Src/GUI.c **** 	else if(delta_x==0)incx=0;//垂直线 
 174:Core/Src/GUI.c **** 	else {incx=-1;delta_x=-delta_x;} 
 175:Core/Src/GUI.c **** 	if(delta_y>0)incy=1; 
 176:Core/Src/GUI.c **** 	else if(delta_y==0)incy=0;//水平线 
 177:Core/Src/GUI.c **** 	else{incy=-1;delta_y=-delta_y;} 
 178:Core/Src/GUI.c **** 	if( delta_x>delta_y)distance=delta_x; //选取基本增量坐标轴 
 179:Core/Src/GUI.c **** 	else distance=delta_y; 
 180:Core/Src/GUI.c **** 	for(t=0;t<=distance+1;t++ )//画线输出 
 181:Core/Src/GUI.c **** 	{  
 182:Core/Src/GUI.c **** 		LCD_DrawPoint(uRow,uCol);//画点 
 183:Core/Src/GUI.c **** 		xerr+=delta_x ; 
 184:Core/Src/GUI.c **** 		yerr+=delta_y ; 
 185:Core/Src/GUI.c **** 		if(xerr>distance) 
 186:Core/Src/GUI.c **** 		{ 
 187:Core/Src/GUI.c **** 			xerr-=distance; 
 188:Core/Src/GUI.c **** 			uRow+=incx; 
 189:Core/Src/GUI.c **** 		} 
 190:Core/Src/GUI.c **** 		if(yerr>distance) 
 191:Core/Src/GUI.c **** 		{ 
 192:Core/Src/GUI.c **** 			yerr-=distance; 
 193:Core/Src/GUI.c **** 			uCol+=incy; 
 194:Core/Src/GUI.c **** 		} 
 195:Core/Src/GUI.c **** 	}  
 196:Core/Src/GUI.c **** } 
