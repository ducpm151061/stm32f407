 355:Core/Src/w25q64.c **** 		}
 356:Core/Src/w25q64.c **** 		else//写入未结束
 357:Core/Src/w25q64.c **** 		{
 358:Core/Src/w25q64.c **** 			secpos++;//扇区地址增1
 359:Core/Src/w25q64.c **** 			secoff=0;//偏移位置为0 	 
 360:Core/Src/w25q64.c **** 		  pBuffer+=secremain;  //指针偏移
 361:Core/Src/w25q64.c **** 			WriteAddr+=secremain;//写地址偏移	   
 362:Core/Src/w25q64.c **** 		  NumByteToWrite-=secremain;				//字节数递减
 363:Core/Src/w25q64.c **** 			if(NumByteToWrite>4096)
 364:Core/Src/w25q64.c **** 			{
 365:Core/Src/w25q64.c **** 				secremain=4096;	//下一个扇区还是写不完
 366:Core/Src/w25q64.c **** 			}
 367:Core/Src/w25q64.c **** 			else 
 368:Core/Src/w25q64.c **** 			{
 369:Core/Src/w25q64.c **** 				secremain=NumByteToWrite;			//下一个扇区可以写完了
 370:Core/Src/w25q64.c **** 			}
 371:Core/Src/w25q64.c **** 		}	 
