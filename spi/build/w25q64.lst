 345:Core/Src/w25q64.c **** 			}
 346:Core/Src/w25q64.c **** 			W25Q64_Write_NoCheck(W25QXX_BUF,secpos*4096,4096);//写入整个扇区  
 347:Core/Src/w25q64.c **** 		}
 348:Core/Src/w25q64.c **** 		else 
 349:Core/Src/w25q64.c **** 		{
 350:Core/Src/w25q64.c **** 			W25Q64_Write_NoCheck(pBuffer,WriteAddr,secremain);//写已经擦除了的,直接写入扇区剩余区间. 				   
 351:Core/Src/w25q64.c **** 		}
 352:Core/Src/w25q64.c **** 		if(NumByteToWrite==secremain)
 353:Core/Src/w25q64.c **** 		{
