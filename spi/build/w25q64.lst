 261:Core/Src/w25q64.c ****  * @name       :void W25Q64_Write_NoCheck(u8* pBuffer,u32 WriteAddr,u16 NumByteToWrite)  
 262:Core/Src/w25q64.c ****  * @date       :2020-05-08 
 263:Core/Src/w25q64.c ****  * @function   :Write SPI flash without check
 264:Core/Src/w25q64.c **** 								You must ensure that all the data in the address range written is 0xff, 
 265:Core/Src/w25q64.c **** 								otherwise the data written in non-0xff will fail!
 266:Core/Src/w25q64.c **** 								With automatic page change function	
 267:Core/Src/w25q64.c **** 								Start to write the specified length data at the specified address, 
 268:Core/Src/w25q64.c **** 								but make sure that the address does not cross the boundary!								
 269:Core/Src/w25q64.c ****  * @parameters :pBuffer:Data storage area
 270:Core/Src/w25q64.c **** 								WriteAddr:Address to start writing (24bit)
 271:Core/Src/w25q64.c **** 								NumByteToWrite:Number of bytes to write (maximum 65535)
 272:Core/Src/w25q64.c ****  * @retvalue   :None
 273:Core/Src/w25q64.c **** ******************************************************************************/	
 274:Core/Src/w25q64.c **** void W25Q64_Write_NoCheck(u8* pBuffer,u32 WriteAddr,u16 NumByteToWrite)   
 275:Core/Src/w25q64.c **** { 			 		 
 276:Core/Src/w25q64.c **** 	u16 pageremain;	   
 277:Core/Src/w25q64.c **** 	pageremain=256-WriteAddr%256; //单页剩余的字节数		 	    
 278:Core/Src/w25q64.c **** 	if(NumByteToWrite<=pageremain)pageremain=NumByteToWrite;//不大于256个字节
 279:Core/Src/w25q64.c **** 	while(1)
 280:Core/Src/w25q64.c **** 	{	   
 281:Core/Src/w25q64.c **** 		W25Q64_Write_Page(pBuffer,WriteAddr,pageremain);
 282:Core/Src/w25q64.c **** 		if(NumByteToWrite==pageremain)
 283:Core/Src/w25q64.c **** 		{
 284:Core/Src/w25q64.c **** 			break;//写入结束了
 285:Core/Src/w25q64.c **** 	 	}
 286:Core/Src/w25q64.c **** 		else //NumByteToWrite>pageremain
 287:Core/Src/w25q64.c **** 		{
 288:Core/Src/w25q64.c **** 			pBuffer+=pageremain;
 289:Core/Src/w25q64.c **** 			WriteAddr+=pageremain;	
 290:Core/Src/w25q64.c **** 
 291:Core/Src/w25q64.c **** 			NumByteToWrite-=pageremain;			  //减去已经写入了的字节数
 292:Core/Src/w25q64.c **** 			if(NumByteToWrite>256)
 293:Core/Src/w25q64.c **** 			{
 294:Core/Src/w25q64.c **** 				pageremain=256; //一次可以写入256个字节
 295:Core/Src/w25q64.c **** 			}
 296:Core/Src/w25q64.c **** 			else 
 297:Core/Src/w25q64.c **** 			{
 298:Core/Src/w25q64.c **** 				pageremain=NumByteToWrite; 	  //不够256个字节了
 299:Core/Src/w25q64.c **** 			}
 300:Core/Src/w25q64.c **** 		}
 301:Core/Src/w25q64.c **** 	}   
 302:Core/Src/w25q64.c **** } 
