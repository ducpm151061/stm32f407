 327:Core/Src/w25q64.c ****  	//printf("ad:%X,nb:%X\r\n",WriteAddr,NumByteToWrite);//测试用
 328:Core/Src/w25q64.c ****  	if(NumByteToWrite<=secremain)secremain=NumByteToWrite;//不大于4096个字节
 329:Core/Src/w25q64.c **** 	while(1) 
 330:Core/Src/w25q64.c **** 	{	
 331:Core/Src/w25q64.c **** 		W25Q64_Read(W25QXX_BUF,secpos*4096,4096);//读出整个扇区的内容
 332:Core/Src/w25q64.c **** 		for(i=0;i<secremain;i++)//校验数据
 333:Core/Src/w25q64.c **** 		{
 334:Core/Src/w25q64.c **** 			if(W25QXX_BUF[secoff+i]!=0XFF)
 335:Core/Src/w25q64.c **** 			{
